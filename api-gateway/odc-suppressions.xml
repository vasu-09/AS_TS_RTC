<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

  <!-- FP: Reactor Netty jars misidentified as Netty (common ODC CPE mapping bug) -->
  <suppress>
    <notes>Reactor Netty falsely mapped to Netty CPE</notes>
    <!-- Make this future-proof: 1.1 or 1.2 lines -->
    <filePath regex="true">.*[/\\]reactor-netty-(core|http)-1\.(1|2)\..*\.jar$</filePath>
    <cpe regex="true">cpe:2\.3:a:(netty|netty_project):netty:.*</cpe>
  </suppress>

  <!-- FP: CVE-2018-1258 reported on Spring Security 6.x -->
  <suppress>
    <notes>Spring Security 6.x false positive</notes>
    <cve>CVE-2018-1258</cve>
    <cpe regex="true">cpe:2\.3:a:(pivotal_software|pivotal):spring_security(_oauth)?:6\..*</cpe>
  </suppress>

</suppressions>
